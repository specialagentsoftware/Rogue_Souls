SceneManager="*res://Autoloads/SceneManager.gd"

[display]

window/size/viewport_width=600
window/size/viewport_height=500
window/size/always_on_top=true
window/size/window_width_override=1200
window/size/window_height_override=1000
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PackedStringArray("res://addons/Todo_Manager/plugin.cfg")

